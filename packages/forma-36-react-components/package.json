  "scripts": {
    "start": "start-storybook -p 9001 -c ./tools/.storybook",
    "test": "jest",
    "build:clean": "rm -rf dist",
    "build:scripts": "BABEL_ENV=build yarn babel src --out-dir dist --ignore *.stories.js,*.test.js",
    "build:styles": "yarn webpack --config ./tools/webpack.production.config.js",
    "build:umd": "rollup -c tools/rollup.config.js",
    "build:post-clean": "rm -rf dist/styles.js dist/main.js",
    "build": "yarn build:clean && yarn build:scripts && yarn build:styles && yarn build:umd && yarn size && yarn build:post-clean",
    "size": "size-limit",
    "generate": "yarn plop --plopfile ./tools/plopfile.js",
    "lint": "yarn eslint src test tools",
    "add-component": "yarn generate component",
    "add-documentation": "yarn generate documentation",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "travis-deploy-once": "travis-deploy-once --pro",
    "semantic-release": "semantic-release",
    "storybook": "yarn build-storybook -c tools/.storybook -o dist-storybook"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/config/tests.js",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/dist-storybook/",
      "<rootDir>/tools/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.css": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful/forma-36.git"
