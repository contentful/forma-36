{
  "name": "@contentful/f36-icons",
  "version": "5.0.0-alpha.5",
  "description": "Forma 36: Icon components",
  "license": "MIT",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "tsx scripts/preBuild.mts && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:cjs": "swc ./src -d ./dist/cjs --strip-leading-paths -C module.type=commonjs",
    "build:esm": "swc ./src -d ./dist/esm --strip-leading-paths",
    "build:types": "tsc --outDir ./dist/types --emitDeclarationOnly"
  },
  "dependencies": {
    "@contentful/f36-core": "^4.63.0",
    "@contentful/f36-icon-alpha": "npm:@contentful/f36-icon@alpha",
    "@contentful/f36-tokens": "^4.0.1",
    "@phosphor-icons/react": "^2.1.4",
    "emotion": "^10.0.17"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "browserslist": "extends @contentful/browserslist-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful/forma-36.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.14",
    "tsx": "^4.7.2"
  }
}
