{
  "name": "@contentful/f36-icon",
  "version": "6.0.0-alpha.0",
  "description": "Forma 36: Icon component",
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "default": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:dual",
    "build:clean": "rm -rf ./dist",
    "build:dual": "tsc -p . && tsc -p ./tsconfig.cjs.json && tsconfig-to-dual-package"
  },
  "dependencies": {
    "@contentful/f36-core": "^6.0.0-alpha.0",
    "@contentful/f36-tokens": "^6.0.0-alpha.0",
    "@phosphor-icons/react": "2.1.8",
    "@emotion/css": "^11.13.5"
  },
  "devDependencies": {
    "react-icons": "^4.4.0",
    "tsconfig-to-dual-package": "^1.2.0"
  },
  "peerDependencies": {
    "react": ">=19.1.0",
    "react-dom": ">=19.1.0"
  },
  "browserslist": "extends @contentful/browserslist-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful/forma-36.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  }
}
