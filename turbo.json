{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "start": {
      "dependsOn": ["^start"]
    },
    "start:prod": {
      "dependsOn": ["^start:prod"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:prod": {
      "dependsOn": ["^build:prod"],
      "outputs": [".next/**"]
    }
  }
}
